{
  "name": "spectator",
  "version": "0.1.0",
  "description": "Local-first Claude JSONL session viewer with project discovery.",
  "license": "MIT",
  "type": "module",
  "bin": {
    "spectator": "dist/node/cli.js"
  },
  "files": [
    "dist",
    "dist/node",
    "public",
    "spectator.config.json",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "dev:server": "bun --hot server.ts",
    "build": "tsc -b && vite build && tsc -p tsconfig.server.json",
    "build:server": "tsc -p tsconfig.server.json",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node dist/node/cli.js",
    "prepack": "npm run build"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "hono": "^4.11.3",
    "@hono/node-server": "^1.13.0",
    "open": "^8.4.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
